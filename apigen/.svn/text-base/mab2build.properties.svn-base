
#MAB2 options for Apigen build

project.name=        apigen
project.longname=    Apigen
project.description= Apigen is an XML-to-Java data binding generator for ADL schemas.  Based \
                     on the XML schemas for an xArch-based ADL, it generates a Java library \
                     that can be used to create and manipulate XML documents conformant \
                     to the given schemas.  The xArch/xADL 2.0 Data Binding Library is \
                     generated using Apigen.
project.toplevelpackages=edu

project.javadoc.params=  -author \
                         -package \
                         -link http://java.sun.com/j2se/1.4/docs/api/

project.manifest=    res/manifest.txt

